    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "module": "manifest.api.app",
            "cwd": "${fileDirname}",
            "justMyCode": true,
            "args": [
                "--model_type", "huggingface",
                "--model_name_or_path", "/local-scratch-nvme/nigam/huggingface/pretrained/gpt-j-6B",
                "--model_generation_type", "text-generation",
                // "--use_hf_parallelize",
                "--device", "0",
                "--port", "5005",
                "--is_flask_debug"
            ]
        }
    ]
}